        "public/lib/mdi/css/materialdesignicons.min.css",
		"public/lib/mdi/css/materialdesignicons.min.css.map",
		"public/lib/mdi/fonts/materialdesignicons-webfont.eot",
		"public/lib/mdi/fonts/materialdesignicons-webfont.svg",
		"public/lib/mdi/fonts/materialdesignicons-webfont.ttf",
		"public/lib/mdi/fonts/materialdesignicons-webfont.woff",
		"public/lib/mdi/fonts/materialdesignicons-webfont.woff2"
      ]

	  
	  
