{
  "name": "Private dashboards",
  "version": "0.0.1",
  "description": "Dashboard ",
  "dependencies": {
    "bootstrap": "~3.1",
	"jquery": "~2.2.1",
	"leaflet" : "~0.7.7",
	"cartodb.js": "~3.15.9",
	"angular" : "~1.5.0",
	"materialize": "*",
	"mdi": "~1.5.54",
	"dcjs": "2.0.0-beta.3",
	"lodash" : "~2.4.1",
	"angular-lodash" : "*",
	"angular-route" : "*",
	"angular-resource" : "*",
	"angular-cookies" : "*",
	"angular-touch" : "*",
	"angular-sanitize" : "*",
	"angular-ui-router" : "~0.2.10",
	"angular-bootstrap" : "*",
	"angular-leaflet-directive" : "0.10.0",
	"leaflet-search" : "~1.8.3",
	"leaflet-ajax" : "~2.0.0",
	"angular-gettext" : "*",
	"Snap.svg" : "*",
	"d3": "*",
	"crossfilter" : "*"
	
  },
  
  "install" : {
        "base":  "public/build/bower",
